<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.heziz.liyang.ui.zhihui.ProjectGKActivity">

    <include layout="@layout/include_title_er"/>
    <CheckBox
        android:id="@+id/cb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/maincolor"
        android:textSize="@dimen/text18"
        android:button="@null"
        android:drawableLeft="@drawable/project_d_name"
        android:drawablePadding="@dimen/padding10"
        android:padding="@dimen/padding10"
        android:text="项目概况"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <include
                layout="@layout/include_project_details"/>
        </LinearLayout>
    </ScrollView>


</LinearLayout>
